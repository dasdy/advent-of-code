{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run t6.ts (ts-node)",
      "runtimeExecutable": "node",
      "runtimeArgs": ["-r", "ts-node/register"],
      "program": "${workspaceFolder}/t6/t6.ts",
      "cwd": "${workspaceFolder}/t6",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run compiled t6.js",
      "program": "${workspaceFolder}/t6/t6.js",
      "cwd": "${workspaceFolder}/t6",
      "console": "integratedTerminal",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true
    }
  ]
}
{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/t6/t6.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}